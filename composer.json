{
    "name": "pyncer/data",
    "type": "library",
    "description": "Pyncer library for handling data.",
    "homepage": "https://pyncer.com/data",
    "license": "Unlicense",
    "require": {
        "php": ">=8.3",
        "psr/http-message": "^2.0",
        "pyncer/core": "^1.1",
        "pyncer/database": "^1.1",
        "pyncer/exception": "^1.0",
        "pyncer/http": "^1.1",
        "pyncer/iterable": "^1.1",
        "pyncer/utility": "^1.0.1",
        "pyncer/validation": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Pyncer\\Data\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/pyncerrc/pyncer-data/issues",
        "source": "https://github.com/pyncerrc/pyncer-data"
    },
    "extra" : {
        "branch-alias" : {
            "dev-main" : "1.4-dev"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.3"
    }
}
